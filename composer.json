{
    "name": "kazist/kazist",
    "type": "project",
    "description": "Description of project kazist.",
    "keywords": ["kazist", "cms", "code generator", "system builder", "ERP", "School Manager"],
    "homepage": "https://github.com/kazist/kazist",
    "license": "GPL-3.0",
    "authors": [ 
        {
            "name": "Dedan Irungu",
            "email": "info@emshyinnovate.com",
            "homepage": "http://emshyinnovate.com",
            "role": "Founder & Developer"
        }
    ],
    "require": {
        "mnsami/composer-custom-directory-installer": "*@stable",
        "composer/composer": "dev-master",
        "kazist/contacts": "*@stable",
        "kazist/media": "*@stable",
        "kazist/notification": "*@stable",
        "kazist/search": "*@stable",
        "kazist/setup": "*@stable",
        "kazist/system": "*@stable",
        "kazist/users": "*@stable",
        "kazist/themes": "*@stable",
        "kazist/core": "*@stable",
        "kazist/assets": "*@stable",
        "symfony/http-foundation": "*@stable",
        "symfony/http-kernel": "*@stable",
        "symfony/event-dispatcher": "*@stable",
        "symfony/dependency-injection": "*@stable",
        "symfony/routing": "*@stable",
        "symfony/yaml": "2.*",
        "symfony/translation": "*@stable",
        "symfony/asset": "*@stable",
        "symfony/finder": "*@stable",
        "symfony/console": "*@stable",
        "symfony/security": "*@stable",
        "symfony/form": "*@stable",
        "symfony/css-selector": "*@stable",
        "symfony/twig-bridge": "*@stable",
        "symfony/config": "*@stable",
        "symfony/validator": "*@stable",
        "symfony/dom-crawler": "*@stable",
        "fabpot/goutte": "*@stable",
        "doctrine/orm": "2.4.*",
        "twig/twig": "1.*@stable",
        "twbs/bootstrap": "3.*@stable",
        "swiftmailer/swiftmailer": "*@stable",
        "mtdowling/cron-expression": "*@stable",
        "kriswallsmith/assetic": "*@stable",
        "tinymce/tinymce": "*@stable",
        "maximebf/debugbar": "*@stable",
        "monolog/monolog": "*@stable",
        "almasaeed2010/adminlte": "*@stable",
        "imagine/imagine": "*@stable",
        "fortawesome/font-awesome": "*@stable",
        "tijsverkoyen/css-to-inline-styles": "*@stable",
        "tecnickcom/tcpdf": "*@stable"
    },
    "scripts": {
        "post-install-cmd": [
            "Setup\\InstallScript::postInstall"
        ],
        "post-update-cmd": [
            "Setup\\InstallScript::postInstall"
        ]
    }, 
    "autoload": {
        "psr-4": {
            "": "include/"
        }
    },
    "extra": {
        "installer-paths": {
            "applications/contacts/": ["kazist/contacts"],
            "applications/media/": ["kazist/media"],
            "applications/notification/": ["kazist/notification"],
            "applications/search/": ["kazist/search"],
            "applications/setup/": ["kazist/setup"],
            "applications/system/": ["kazist/system"],
            "applications/users/": ["kazist/users"],
            "assets/": ["kazist/assets"],
            "themes/": ["kazist/themes"],
            "include/": ["kazist/core"]
        }
    }
}
