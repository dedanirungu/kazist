<?php

namespace Users\Permission\Code;

use Doctrine\ORM\Mapping as ORM;

/**
 * Permission
 *
 * @ORM\Table(name="users_permission")
 * @ORM\Entity
 */
class Permission extends \Kazist\Table\BaseTable
{
    /**
     * @var integer
     *
     * @ORM\Column(name="id", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="IDENTITY")
     */
    private $id;

    /**
     * @var integer
     *
     * @ORM\Column(name="app_id", type="integer", length=11)
     */
    private $app_id;

    /**
     * @var integer
     *
     * @ORM\Column(name="com_id", type="integer", length=11)
     */
    private $com_id;

    /**
     * @var integer
     *
     * @ORM\Column(name="subset_id", type="integer", length=11)
     */
    private $subset_id;

    /**
     * @var integer
     *
     * @ORM\Column(name="role_id", type="integer", length=11)
     */
    private $role_id;

    /**
     * @var integer
     *
     * @ORM\Column(name="view", type="integer", length=11)
     */
    private $view;

    /**
     * @var integer
     *
     * @ORM\Column(name="add", type="integer", length=11)
     */
    private $add;

    /**
     * @var integer
     *
     * @ORM\Column(name="write", type="integer", length=11)
     */
    private $write;

    /**
     * @var integer
     *
     * @ORM\Column(name="delete", type="integer", length=11)
     */
    private $delete;

    /**
     * @var integer
     *
     * @ORM\Column(name="viewown", type="integer", length=11)
     */
    private $viewown;

    /**
     * @var integer
     *
     * @ORM\Column(name="writeown", type="integer", length=11)
     */
    private $writeown;

    /**
     * @var integer
     *
     * @ORM\Column(name="deleteown", type="integer", length=11)
     */
    private $deleteown;

    /**
     * @var integer
     *
     * @ORM\Column(name="created_by", type="integer", length=11)
     */
    private $created_by;

    /**
     * @var \DateTime
     *
     * @ORM\Column(name="date_created", type="datetime")
     */
    private $date_created;

    /**
     * @var integer
     *
     * @ORM\Column(name="modified_by", type="integer", length=11)
     */
    private $modified_by;

    /**
     * @var \DateTime
     *
     * @ORM\Column(name="date_modified", type="datetime")
     */
    private $date_modified;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set app_id
     *
     * @param integer $appId
     * @return Permission
     */
    public function setAppId($appId)
    {
        $this->app_id = $appId;

        return $this;
    }

    /**
     * Get app_id
     *
     * @return integer 
     */
    public function getAppId()
    {
        return $this->app_id;
    }

    /**
     * Set com_id
     *
     * @param integer $comId
     * @return Permission
     */
    public function setComId($comId)
    {
        $this->com_id = $comId;

        return $this;
    }

    /**
     * Get com_id
     *
     * @return integer 
     */
    public function getComId()
    {
        return $this->com_id;
    }

    /**
     * Set subset_id
     *
     * @param integer $subsetId
     * @return Permission
     */
    public function setSubsetId($subsetId)
    {
        $this->subset_id = $subsetId;

        return $this;
    }

    /**
     * Get subset_id
     *
     * @return integer 
     */
    public function getSubsetId()
    {
        return $this->subset_id;
    }

    /**
     * Set role_id
     *
     * @param integer $roleId
     * @return Permission
     */
    public function setRoleId($roleId)
    {
        $this->role_id = $roleId;

        return $this;
    }

    /**
     * Get role_id
     *
     * @return integer 
     */
    public function getRoleId()
    {
        return $this->role_id;
    }

    /**
     * Set view
     *
     * @param integer $view
     * @return Permission
     */
    public function setView($view)
    {
        $this->view = $view;

        return $this;
    }

    /**
     * Get view
     *
     * @return integer 
     */
    public function getView()
    {
        return $this->view;
    }

    /**
     * Set add
     *
     * @param integer $add
     * @return Permission
     */
    public function setAdd($add)
    {
        $this->add = $add;

        return $this;
    }

    /**
     * Get add
     *
     * @return integer 
     */
    public function getAdd()
    {
        return $this->add;
    }

    /**
     * Set write
     *
     * @param integer $write
     * @return Permission
     */
    public function setWrite($write)
    {
        $this->write = $write;

        return $this;
    }

    /**
     * Get write
     *
     * @return integer 
     */
    public function getWrite()
    {
        return $this->write;
    }

    /**
     * Set delete
     *
     * @param integer $delete
     * @return Permission
     */
    public function setDelete($delete)
    {
        $this->delete = $delete;

        return $this;
    }

    /**
     * Get delete
     *
     * @return integer 
     */
    public function getDelete()
    {
        return $this->delete;
    }

    /**
     * Set viewown
     *
     * @param integer $viewown
     * @return Permission
     */
    public function setViewown($viewown)
    {
        $this->viewown = $viewown;

        return $this;
    }

    /**
     * Get viewown
     *
     * @return integer 
     */
    public function getViewown()
    {
        return $this->viewown;
    }

    /**
     * Set writeown
     *
     * @param integer $writeown
     * @return Permission
     */
    public function setWriteown($writeown)
    {
        $this->writeown = $writeown;

        return $this;
    }

    /**
     * Get writeown
     *
     * @return integer 
     */
    public function getWriteown()
    {
        return $this->writeown;
    }

    /**
     * Set deleteown
     *
     * @param integer $deleteown
     * @return Permission
     */
    public function setDeleteown($deleteown)
    {
        $this->deleteown = $deleteown;

        return $this;
    }

    /**
     * Get deleteown
     *
     * @return integer 
     */
    public function getDeleteown()
    {
        return $this->deleteown;
    }

    /**
     * Set created_by
     *
     * @param integer $createdBy
     * @return Permission
     */
    public function setCreatedBy($createdBy)
    {
        $this->created_by = $createdBy;

        return $this;
    }

    /**
     * Get created_by
     *
     * @return integer 
     */
    public function getCreatedBy()
    {
        return $this->created_by;
    }

    /**
     * Set date_created
     *
     * @param \DateTime $dateCreated
     * @return Permission
     */
    public function setDateCreated($dateCreated)
    {
        $this->date_created = $dateCreated;

        return $this;
    }

    /**
     * Get date_created
     *
     * @return \DateTime 
     */
    public function getDateCreated()
    {
        return $this->date_created;
    }

    /**
     * Set modified_by
     *
     * @param integer $modifiedBy
     * @return Permission
     */
    public function setModifiedBy($modifiedBy)
    {
        $this->modified_by = $modifiedBy;

        return $this;
    }

    /**
     * Get modified_by
     *
     * @return integer 
     */
    public function getModifiedBy()
    {
        return $this->modified_by;
    }

    /**
     * Set date_modified
     *
     * @param \DateTime $dateModified
     * @return Permission
     */
    public function setDateModified($dateModified)
    {
        $this->date_modified = $dateModified;

        return $this;
    }

    /**
     * Get date_modified
     *
     * @return \DateTime 
     */
    public function getDateModified()
    {
        return $this->date_modified;
    }
}
